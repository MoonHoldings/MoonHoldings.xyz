{
  "name": "moonholdings.xyz",
  "version": "1.0.0",
  "description": "The Web3 Portfolio Intelligence App.",
  "author": "Leon <leon@moonholdings.xyz> (https://twitter.com/leongaban)",
  "contributors": [
    "Leon Gaban",
    "Abeer Mithela",
    "Ajoy Thomas"
  ],
  "scripts": {
    "build": "vite build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint . --ext .vue,.js,.jsx, --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "coverage": "jest --coverage",
    "cypress": "cypress run --headed --no-exit",
    "cypress-headless": "cypress run",
    "dev": "vite",
    "prettier": "prettier --write .",
    "preview": "vite preview --port 4173",
    "test": "NODE_ENV=test jest --no-cache",
    "test-watch": "NODE_ENV=test jest --watch --no-cache"
  },
  "dependencies": {
    "@solana/wallet-adapter-wallets": "^0.19.5",
    "@vueup/vue-quill": "^1.0.0-beta.10",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "firebase": "^9.9.4",
    "jwt-decode": "^3.1.2",
    "mixpanel-browser": "^2.45.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^2.3.0",
    "solana-wallets-vue": "^0.5.2",
    "vue": "^3.2.36",
    "vue-chartjs": "^4.1.2",
    "vue-router": "^4.0.15",
    "vue3-cookies": "^1.0.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "c8": "^7.11.3",
    "cypress": "^10.4.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "^27.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ramda": "^0.28.0",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.9.9"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.vue?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/utils/"
    ]
  },
  "homepage": "https://moonholdings.xyz"
}
